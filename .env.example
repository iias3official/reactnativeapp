AGORA_APP_CERTIFICATE=your_agora_certificate_here

# Error Tracking - Sentry (Optional)
# Get DSN from: https://sentry.io/
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development

# Environment
NODE_ENV=development

# App Configuration
