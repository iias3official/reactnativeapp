{
  "expo": {
    "name": "AstroWisdom",
    "slug": "astrowisdom",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFF5F8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lumora.astrowisdom"
    },
    "android": {
      "package": "com.lumora.astrowisdom",
      "adaptiveIcon": {
        "backgroundColor": "#FFF5F8"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/messaging"
    ]
  }
}
